CONST MAX = 100;
VAR ARG, RET;

PROCEDURE ISPRIME;
VAR I;
BEGIN
	RET := 1;
	I := 2;
	WHILE I < ARG DO
	BEGIN
		IF ARG / I * I = ARG THEN
		BEGIN
			RET := 0;
			I := ARG
		END;
		I := I + 1
	END
END;

PROCEDURE PRIMES;
BEGIN
	ARG := 2;
	WHILE ARG < MAX DO
	BEGIN
		CALL ISPRIME;
		IF RET = 1 THEN ! ARG;
		ARG := ARG + 1
	END
END;

CALL PRIMES
.